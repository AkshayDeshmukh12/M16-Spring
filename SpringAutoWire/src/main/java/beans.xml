<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context" xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd"> <!-- bean definitions here -->


<context:annotation-config></context:annotation-config>
<bean id="h" class="com.tns.springaw.Heart">
<property name="nameofAnimal" value="Tiger"></property>
<property name="nameofHeart" value="2"></property>
</bean>
<bean id="h1" class="com.tns.springaw.Heart">
<property name="nameofAnimal" value="Human"></property>
<property name="nameofHeart" value="1"></property>
</bean>
<bean id="human" class="com.tns.springaw.Human" autowire="constructor"></bean>

</beans>